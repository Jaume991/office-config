#Android Path
export PATH=/usr/local/android-studio/bin:$PATH
export ANDROID_HOME=~/Android/Sdk
export PATH=${PATH}:${ANDROID_HOME}/tools
export PATH=${PATH}:${ANDROID_HOME}/platform-tools
# Dotfiles
export DOTPATH=/home/[user]
export HOSTSPATH=/etc/hosts
# Flycheck php
export PATH=~/.composer/vendor/bin:$PATH
export PHPMD=/home/[user]/.composer/vendor/bin/phpmd
export PHPCS=/home/[user]/.composer/vendor/bin/phpcs
export PHPCS_RULESET=/home/[user]/.composer/vendor/pragmarx/laravelcs/Standards/Laravel/ruleset.xml
# Spacemacs
export YCMDPATH=/home/[user]/ycmd/ycmd
